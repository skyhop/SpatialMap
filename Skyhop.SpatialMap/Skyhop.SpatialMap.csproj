<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Authors>Corstian Boerman</Authors>
    <Company>Whaally</Company>
    <PackageProjectUrl>https://github.com/skyhop/SpatialMap</PackageProjectUrl>
    <RepositoryUrl>https://github.com/skyhop/SpatialMap</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>This package contains a data type which supports projection of an object onto a 2D X/Y grid. It is meant to quickly locate objects around a certain coordinate, and doesn't support lookup by values. This object is implemented on top of the SortedList type and therefore uses a binary search for object location.</Description>
    <Copyright>(c) Whaally 2020</Copyright>
    <Version>1.0.1</Version>
  </PropertyGroup>

</Project>
